{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "baseUrl": "./",

    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true
    },
    "target": "es2022",
    "experimental": {
      "keepImportAssertions": true
    }
  },
  "module": { "type": "es6", "resolveFully": true },
  "sourceMaps": false,
  "isModule": true
}
